{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./types/**/*", "./gatsby-node/**/*", "./src/**/*"]
}
